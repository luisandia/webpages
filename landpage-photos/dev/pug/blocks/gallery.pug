.gallery
  h2.gallery__title.section__title Galeria
  nav.gallery__nav
    ul.menu--section
      li.menu__item: a(href="#").menu__link Todo
      li.menu__item: a(href="#").menu__link Gente
      li.menu__item: a(href="#").menu__link Lugares
      li.menu__item: a(href="#").menu__link Naturaleza

  .imag-gallery
    -for(var i=1;i<13;i++)
      .image-gallery__item
        if i < 10
          img(src="img/0"+i+".jpg").image-gallery__image
        else
          img(src="img/"+i+".jpg").image-gallery__image
